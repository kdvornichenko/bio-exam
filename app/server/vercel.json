{
	"installCommand": "corepack enable && yarn --cwd ../.. install",
	"buildCommand": "yarn --cwd ../.. workspace @bio-exam/rbac build && yarn --cwd ../.. workspace @bio-exam/server build",
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api/index.ts"
		},
		{
			"source": "/healthz",
			"destination": "/api/index.ts"
		}
	],
	"functions": {
		"api/index.ts": {
			"maxDuration": 30,
			"memory": 1024
		}
	}
}
